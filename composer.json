	"autoload": {
		"psr-4": {
			"Iqbalatma\\LaravelServiceRepo\\": "src/"
		},
        "files": [
            "src/helper.php"
        ]
	},
	"authors": [
		{
			"name": "iqbalatma",
			"email": "iqbalatma@gmail.com"
		}
	],
	"require": {
		"php": ">=8.0"
	},
    "extra": {
        "laravel": {
            "providers": [
                "Iqbalatma\\LaravelServiceRepo\\ServiceRepoProvider"
            ]
        }
    }
}
